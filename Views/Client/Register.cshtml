@model MercadoEsquina.Models.Client
@{
    ViewData["Title"] = "Cadastro de Clientes";
}

<div class="container">
    <h2>Novo Cliente</h2>
    <form>
        <div class="form-group">
            <label for="email">Id:</label>
            <input type="email" class="form-control" id="email" placeholder="Digite ID -> Temporário">
        </div>
        <div class="form-group">
            <label for="pwd">Nome:</label>
            <input type="password" class="form-control" id="pwd" placeholder="Digite seu nome">
        </div>
        <div class="form-group">
            <label for="pwd">Cpf:</label>
            <input type="password" class="form-control" id="pwd" placeholder="Digite seu cpf">
        </div>
        <div class="form-group">
            <label for="pwd">Data de Nascimento:</label>
            <input type="password" class="form-control" id="pwd" placeholder="Digite sua data de nascimento">
        </div>
        <button type="submit" class="btn btn-default" value="Register">Submit</button>
    </form>
</div>